{
  "source": {
    "include": [
      "./lib",
      "README.md"
    ],
    "exclude": [
      "./node_modules"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "./node_modules/ink-docstrap/template",
    "verbose": true
  },
  "plugins": [
    "plugins/markdown"
  ]
}
