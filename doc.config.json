{
  "source": {
    "include": [
      "./lib",
      "./README.md"
    ],
    "exclude": [
      "./node_modules"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "./docs",
    "readme": "./README.md",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "dark",
      "homepageTitle": "wlLive2d",
      "title": "wlLive2d",
      "meta": [
        {
          "name": "author",
          "content": "wonder-light"
        },
        {
          "name": "description",
          "content": "Best Clean and minimal JSDoc 3 Template/Theme"
        }
      ]
    }
  }
}
