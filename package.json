{
  "name": "wl-live2d",
  "version": "1.0.7-rc.1",
  "description": "Live2D for web, 这是一个运行在浏览器环境的 Live2D 看板组件",
  "author": "wonder-light",
  "license": "MIT",
  "keywords": [
    "live2d",
    "wl-live2d",
    "cubism2",
    "cubism4",
    "web",
    "component"
  ],
  "homepage": "https://wonder-light.github.io/wl-live2d/",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "unpkg": "./dist/umd/index.js",
  "jsdelivr": "./dist/cjs/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/cjs/index.js",
      "require": "./dist/cjs/index.js",
      "import": "./dist/es/index.js",
      "types": "./dist/index.d.ts"
    },
    "./es": "./dist/es/index.js",
    "./cjs": "./dist/cjs/index.js",
    "./umd": "./dist/umd/index.js",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wonder-light/wl-live2d.git"
  },
  "bugs": {
    "url": "https://github.com/wonder-light/wl-live2d/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "npm --prefix test/app run dev || pnpm -F test/app run dev",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "docs:dev": "rspress dev",
    "docs:build": "rspress build",
    "docs:preview": "rspress preview"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "pixi-live2d-display-advanced": "^0.5.4",
    "pixi.js": "^7.1.2",
    "ramda": "^0.29.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@pixi/app": "^7.1.2",
    "@pixi/core": "^7.1.2",
    "@pixi/display": "^7.1.2",
    "@pixi/ticker": "^7.1.2",
    "@pixi/utils": "^7.1.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/ramda": "^0.31.0",
    "autoprefixer": "^10.4.17",
    "clean-jsdoc-theme": "^4.2.17",
    "cssnano": "^6.0.5",
    "jsdoc": "^4.0.2",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.35",
    "postcss-nested": "^6.0.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rspress": "^1.45.1",
    "typescript": "^5.7.2"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  }
}
